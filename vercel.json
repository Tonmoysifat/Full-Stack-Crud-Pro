{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "env": {
    "MONGODB_URI": "mongodb://localhost:27017/Full-stack-crud-pro"
  }
}
